<Page x:Class="BaggageSortertingSystemWpf.SidePanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BaggageSortertingSystemWpf"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="200"
      Title="SidePanel">

    <Grid Background="#4F6367">
        <Label x:Name="Clock"></Label>
        <Button x:Name="Dashboard" Content="Dashboard" HorizontalAlignment="Center" Height="30" Margin="0,22,0,0" VerticalAlignment="Top" Width="180" BorderBrush="{x:Null}" Foreground="White" Click="Dashboard_Click"/>
       
        <Button x:Name="FlyingPlanButton" Content="Flyingplan" HorizontalAlignment="Center" Height="30" Margin="0,67,0,0" VerticalAlignment="Top" Width="180" BorderBrush="{x:Null}" Background="White" Foreground="White" Click="FlyingPlanButton_Click"/>

    </Grid>
</Page>
